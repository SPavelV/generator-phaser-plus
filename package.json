{
  "name": "generator-phaser-plus",
  "description": "Create Phaser Web games using Browserify, BrowserSync, Gulp and more.",
  "version": "2.0.1",
  "homepage": "https://github.com/rblopes/generator-phaser-plus#readme",
  "author": "Rafael Barbosa Lopes (https://github.com/rblopes)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rblopes/generator-phaser-plus.git"
  },
  "bugs": "https://github.com/rblopes/generator-phaser-plus/issues",
  "keywords": [
    "yeoman-generator",
    "babel",
    "canvas",
    "es2015",
    "es2016",
    "es2017",
    "es6",
    "es7",
    "esnext",
    "game development",
    "gamedev",
    "games",
    "html5",
    "phaser",
    "scaffold",
    "webgl"
  ],
  "files": [
    "generators/",
    "lib/"
  ],
  "scripts": {
    "fix": "xo --fix",
    "lint": "xo",
    "test": "NODE_ENV=test mocha --reporter spec"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.trim": "^4.5.0",
    "lodash.upperfirst": "^4.3.0",
    "yeoman-generator": "^1.1.0"
  },
  "devDependencies": {
    "fs-extra": "^2.1.0",
    "mocha": "^3.2.0",
    "xo": "^0.19.0",
    "yeoman-assert": "^2.2.0",
    "yeoman-test": "^1.6.0"
  },
  "xo": {
    "envs": [
      "mocha",
      "node"
    ],
    "esnext": true,
    "ignores": [
      "generators/*/templates/**",
      "test/fixtures/**"
    ],
    "rules": {
      "capitalized-comments": "off"
    },
    "space": true
  },
  "license": "MIT"
}
