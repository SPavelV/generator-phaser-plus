{
  "name": "generator-phaser-plus",
  "description": "A Yeoman generator for Phaser web games, featuring a live environment with Babel, BrowserSync, Browserify and much more.",
  "version": "0.3.1",
  "homepage": "https://github.com/rblopes/generator-phaser-plus#readme",
  "author": "Rafael Barbosa Lopes <rafael.barbosalopes@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rblopes/generator-phaser-plus.git"
  },
  "bugs": "https://github.com/rblopes/generator-phaser-plus/issues",
  "keywords": [
    "yeoman-generator",
    "es2015",
    "es6",
    "gamedev",
    "phaser",
    "scaffold"
  ],
  "scripts": {
    "lint": "xo",
    "test": "NODE_ENV=test mocha test/*.js --reporter spec"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash.camelcase": "^4.0.1",
    "lodash.flowright": "^3.2.1",
    "lodash.isempty": "^4.1.1",
    "lodash.trim": "^4.0.2",
    "lodash.upperfirst": "^4.0.2",
    "yeoman-generator": "^0.22.0"
  },
  "devDependencies": {
    "mocha": "^2.3.0",
    "xo": "*",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  },
  "xo": {
    "space": true,
    "envs": [
      "node",
      "mocha"
    ],
    "ignores": [
      "generators/*/templates/**"
    ]
  },
  "license": "MIT"
}
